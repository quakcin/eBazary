{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"justify-around": {
		"style": {
			"justifyContent": "space-around"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"p-3": {
		"style": {
			"paddingTop": 12,
			"paddingRight": 12,
			"paddingBottom": 12,
			"paddingLeft": 12
		}
	}
}